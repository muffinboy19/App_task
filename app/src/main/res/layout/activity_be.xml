<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F6F6F6"
    tools:context=".be">

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="104dp"
        android:background="@drawable/rounded_corner_view"
        android:backgroundTint="#f6f6f6"
        android:orientation="vertical"
        android:paddingHorizontal="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="72dp"
            android:layout_weight="0"
            android:paddingHorizontal="20dp"
            android:text="Bay Beach"
            android:textColor="#8AC3AE"
            android:textSize="48sp"
            android:textStyle="normal" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="match_parent"
            android:layout_height="36dp"
            android:layout_weight="0"
            android:paddingHorizontal="20dp"
            android:text="Mexico"
            android:textColor="#8AC3AE"
            android:textSize="24sp"
            android:textStyle="normal" />

    </LinearLayout>

    <ImageButton
        android:id="@+id/ib"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/extra_round"
        android:backgroundTint="#0338F4"
        android:paddingHorizontal="5dp"
        android:paddingVertical="5dp"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.589"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/arrrr" />

    <ImageView
        android:id="@+id/imv2"
        android:layout_width="match_parent"
        android:layout_height="550dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/b1" />

    <ImageButton
        android:id="@+id/flower2"
        android:layout_width="72dp"
        android:layout_height="72dp"
        android:layout_marginBottom="248dp"
        android:background="@drawable/extra_round"
        android:backgroundTint="#00E5FF"
        android:paddingHorizontal="10dp"
        android:paddingVertical="10dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.952"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/p" />

    <ImageButton
        android:id="@+id/side2"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_marginBottom="464dp"
        android:background="@drawable/extra_round"
        android:backgroundTint="#0000"
        android:paddingHorizontal="5dp"
        android:paddingVertical="5dp"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.955"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/x" />

    <ImageButton
        android:id="@+id/back2"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/extra_round"
        android:backgroundTint="#0338F4"
        android:paddingHorizontal="6dp"
        android:paddingVertical="6dp"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.371"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ubu" />
</androidx.constraintlayout.widget.ConstraintLayout>